<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:viewmodel="clr-namespace:MusicMediaPlayer.ViewModel"
                    xmlns:converter="clr-namespace:MusicMediaPlayer.Converter">
    <viewmodel:MainViewModel x:Key="MVApp"></viewmodel:MainViewModel>
    <viewmodel:LoginViewModel x:Key="LoginVM"></viewmodel:LoginViewModel>
    <viewmodel:RegisterViewModel x:Key="RegisterVM"></viewmodel:RegisterViewModel>
    <viewmodel:SongViewModel x:Name="SongVM" x:Key="SongVM"></viewmodel:SongViewModel>
    <converter:ByteArrayToBitmapImageConverter x:Name="binaryConverter" x:Key="ByteToImageConverter"/>
    <!-- Dưới này là của tao - Kiet -->
    <viewmodel:PlayListViewModel x:Key="PlayListVM"></viewmodel:PlayListViewModel>
    <viewmodel:AddPlayListViewModel x:Key="AddPlayListVM"></viewmodel:AddPlayListViewModel>
    <viewmodel:RenamePlayListViewModel x:Key="RenamePlayListVM"></viewmodel:RenamePlayListViewModel>
    <viewmodel:PlayList_InsideViewModel x:Key="PlayList_InsideVM"></viewmodel:PlayList_InsideViewModel>
    <viewmodel:AddSongPlayListViewModel x:Key="AddSongPlayListVM"></viewmodel:AddSongPlayListViewModel>
</ResourceDictionary>