                    xmlns:vm="clr-namespace:MusicMediaPlayer.ViewModel"
                    xmlns:view="clr-namespace:MusicMediaPlayer.UserControlMusic"
                    xmlns:v ="clr-namespace:MusicMediaPlayer.View">
    <DataTemplate DataType="{x:Type vm:UCHomeViewModel}">
        <view:UserControlHome/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:UCProfileViewModel}">
        <view:UserControlProFile/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type vm:LogoutViewModel}">
        <v:Logout></v:Logout>
    </DataTemplate>
</ResourceDictionary>